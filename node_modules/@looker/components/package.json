  "src": "src/index.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "url": "looker-open-source/components",
    "directory": "packages/components"
  },
  "files": [
